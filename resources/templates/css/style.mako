body {
    padding: 1rem;
    margin: 5% 30%;
    display: grid;
}

.DivItem {
    display: grid;
    margin: 0
    padding: 0
    grid-template-columns: 1fr 3fr;
    grid-gap: 10px;

}

.Div {
    display: grid;
    padding: 1em;
    grid-template-columns: 1fr;
    grid-gap: 10px;
    margin: 5% 10%;

}

.LWindow {
    display: flex;
    flex-direction: column;
}

.Label {
    text-align: left;
 }

